@(ingredient: Ingredient)

<ingredient>
    <id>@ingredient.getId()</id>
    <name>@ingredient.getName()</name>
    <description>@ingredient.getDescription()</description>
    <price>@ingredient.getPrice()</price>
    <coin>@ingredient.getCoin()</coin>
    <whenCreated>@ingredient.getWhenCreated()</whenCreated>
    <whenUpdated>@ingredient.getWhenUpdated()</whenUpdated>
</ingredient>